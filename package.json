{
  "name": "stac-layer",
  "version": "0.0.0",
  "description": "Visualize a STAC Item or Collection on a Leaflet Map",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "type": "module",
  "scripts": {
    "build": "webpack",
    "dev": "concurrently \"npx snowpack build --watch\" \"cd build && npx http-server\"",
    "format": "npx prettier --arrow-parens=avoid --print-width=100 --trailing-comma=none --write *.js */*.js",
    "serve": "HOST=0.0.0.0 http-server --cors",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJDufour/stac-layer.git"
  },
  "keywords": [
    "cog",
    "geojson",
    "geotiff",
    "jpg",
    "png",
    "shapefile",
    "stac"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/stac-layer/issues"
  },
  "homepage": "https://github.com/DanielJDufour/stac-layer#readme",
  "dependencies": {
    "chroma-js": "^1.4.1",
    "georaster-layer-for-leaflet": "^2.0.0",
    "get-depth": "^0.0.1",
    "reproject-bbox": "^0.0.1"
  },
  "devDependencies": {
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "georaster": "^1.5.4",
    "http-server": "^0.12.3",
    "parse-filename": "^0.0.0",
    "snowpack": "^3.7.1",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  }
}
